<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardBackgroundColor="@color/color_white"
    app:cardCornerRadius="8dp"
    android:layout_margin="10dp">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="15dp" />
   <LinearLayout
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:orientation="horizontal"
       android:layout_marginStart="10dp"
       android:layout_marginEnd="10dp">
       <atiaf.mehany.Customecalss.TextViewWithFont
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/per"
           android:textSize="8pt"
           android:textColor="@color/color_back_dark"/>
       <atiaf.mehany.Customecalss.TextViewWithFont
           android:layout_marginStart="10dp"
           android:id="@+id/num"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/per"
           android:textSize="8pt"
           android:textColor="@color/color_back_dark"/>
   </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/per1"
            android:textSize="8pt"
            android:textColor="@color/color_back_dark"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_marginStart="10dp"
            android:id="@+id/date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/per"
            android:textSize="8pt"
            android:textColor="@color/color_back_dark"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="20dp">
        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:id="@+id/img"/>
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:id="@+id/name"
            android:textSize="8pt"
            android:textColor="@color/color_back_dar"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order6"
            android:textSize="8pt"
            android:textColor="@color/color_back_dark"/>
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order4"
            android:layout_marginStart="10dp"
            android:id="@+id/loc"
            android:textSize="8pt"
            android:textColor="@color/color_grey"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/li"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order10"
            android:textSize="8pt"
            android:textColor="@color/color_back_dark"/>
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order4"
            android:layout_marginStart="10dp"
            android:id="@+id/ordertype"
            android:textSize="8pt"
            android:textColor="@color/color_grey"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:id="@+id/lin">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order8"
            android:textSize="8pt"
            android:textColor="@color/color_back_dark"/>
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order4"
            android:layout_marginStart="10dp"
            android:id="@+id/stage"
            android:textSize="8pt"
            android:textColor="@color/color_grey"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:id="@+id/lin1">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order9"
            android:textSize="8pt"
            android:textColor="@color/color_back_dark"/>
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order4"
            android:layout_marginStart="10dp"
            android:id="@+id/subject"
            android:textSize="8pt"
            android:textColor="@color/color_grey"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:id="@+id/lin2">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/worknot2"
            android:textSize="8pt"
            android:textColor="@color/color_back_dark"/>
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order4"
            android:layout_marginStart="10dp"
            android:id="@+id/sertype"
            android:textSize="8pt"
            android:textColor="@color/color_grey"/>
    </LinearLayout>
    <atiaf.mehany.Customecalss.TextViewWithFont
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/detailsorder14"
        android:textSize="8pt"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:textColor="@color/color_back_dark"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="@string/detailsorder15"
            android:textSize="7pt"
            android:layout_gravity="center"
            android:textColor="@color/color_back_dark"/>
        <TextView
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            />
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t1"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t2"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t3"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t4"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t5"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp">
        <atiaf.mehany.Customecalss.TextViewWithFont
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="@string/detailsorder16"
            android:textSize="7pt"
            android:layout_gravity="center"
            android:textColor="@color/color_back_dark"/>
        <TextView
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            />
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t11"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t12"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t13"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t14"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
        <TextView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:id="@+id/t15"
            android:background="@drawable/mrb_star_border_icon_black_36dp"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="15dp" />
</LinearLayout>
</androidx.cardview.widget.CardView>