<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:openDrawer="start"
    android:fitsSystemWindows="true"
    android:orientation="vertical">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="200dp">
                <fragment
                    android:id="@+id/maps"
                    android:name="com.google.android.gms.maps.SupportMapFragment"
                    android:layout_width="fill_parent"
                    android:layout_height="200dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_gps_fixed"
                    android:layout_gravity="start|bottom"
                    android:layout_marginStart="20dp"
                    android:layout_marginBottom="60dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="15dp"
                            android:layout_height="15dp" />
                        <TextView
                            android:id="@+id/menu"
                            android:layout_gravity="center"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/ic_menu"/>
                        <atiaf.mehany.Customecalss.TextViewWithFont
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:textSize="8pt"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textColor="@color/color_back_dark"
                            android:text="@string/selloc"
                            android:singleLine="true"/>
                        <TextView
                            android:layout_width="15dp"
                            android:layout_height="15dp" />
                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="horizontal"
                        android:background="#40000000">
                        <TextView
                            android:layout_width="15dp"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/rec"
                            android:layout_gravity="center"/>
                        <TextView
                            android:layout_width="15dp"
                            android:layout_height="wrap_content" />
                        <atiaf.mehany.Customecalss.TextViewWithFont
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:textSize="8pt"
                            android:layout_gravity="center"
                            android:textColor="@color/color_white"
                            android:id="@+id/loc"
                            android:singleLine="true"/>
                        <TextView
                            android:layout_width="15dp"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:visibility="gone"
                        android:id="@+id/lin"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"/>
                            <TextView
                                android:id="@+id/icon"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:background="@drawable/ic_teacher_2"/>
                            <TextView
                                android:layout_width="15dp"
                                android:layout_height="wrap_content" />
                            <atiaf.mehany.Customecalss.TextViewWithFont
                                android:id="@+id/txt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="8pt"
                                android:layout_gravity="center"
                                android:textColor="@color/color_blue1"
                                android:text="@string/main4"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"/>
                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="5dp"
                            android:background="@color/color_blue1"
                            android:id="@+id/t1"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:visibility="gone"
                        android:background="@color/color_gray2"/>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/lin1"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"/>
                            <TextView
                                android:id="@+id/icon1"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_gravity="center"
                                android:background="@drawable/ic_worker_1"/>
                            <TextView
                                android:layout_width="15dp"
                                android:layout_height="wrap_content" />
                            <atiaf.mehany.Customecalss.TextViewWithFont
                                android:id="@+id/txt1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="8pt"
                                android:layout_gravity="center"
                                android:textColor="@color/color_back_dar"
                                android:text="@string/main5"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"/>
                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="5dp"
                            android:background="@color/color_gray2"
                            android:id="@+id/t2"/>
                    </LinearLayout>

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/color_gray2"/>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/lin_services"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"/>
                            <TextView
                                android:id="@+id/icon1_services"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:visibility="gone"
                                android:layout_gravity="center"
                                android:background="@drawable/ic_worker_1"/>
                            <TextView
                                android:layout_width="15dp"
                                android:layout_height="wrap_content" />
                            <atiaf.mehany.Customecalss.TextViewWithFont
                                android:id="@+id/txt1_services"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="8pt"
                                android:layout_gravity="center"
                                android:textColor="@color/color_back_dar"
                                android:text="@string/other_services"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"/>
                        </LinearLayout>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="5dp"
                            android:background="@color/color_gray2"
                            android:id="@+id/t2_services"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_gray2"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1">
                    <include android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        layout="@layout/teacher"/>
                    <include android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        layout="@layout/technical"/>
                </FrameLayout>
            </LinearLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <atiaf.mehany.Customecalss.TextViewWithFont
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="@color/color_blue1"
                    android:textSize="8pt"
                    android:gravity="center"
                    android:id="@+id/sendorder"
                    android:text="@string/sendorder2"
                    android:textColor="@color/color_white"/>
                <LinearLayout
                    android:background="@color/color_blue1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:id="@+id/linsearch"
                    android:visibility="gone"
                    >
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content" />
                    <atiaf.mehany.Customecalss.TextViewWithFont
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:background="@color/color_blue1"
                        android:textSize="8pt"
                        android:gravity="center"
                        android:text="@string/sendorder7"
                        android:textColor="@color/color_white"/>
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"/>

                    <atiaf.mehany.Customecalss.TextViewWithFont
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:background="@color/color_blue1"
                        android:textSize="8pt"
                        android:gravity="center"
                        android:text="@string/sendorder8"
                        android:textColor="@color/color_white"
                        android:id="@+id/view"
                        />
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content" />
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
        <include android:layout_height="match_parent"
            android:layout_width="match_parent"
            layout="@layout/success"></include>
    </FrameLayout>
  <include android:layout_height="match_parent"
      android:layout_width="match_parent"
      layout="@layout/menu"
      android:visibility="gone"
      android:gravity="start"
      android:layout_gravity="start"
      ></include>


</androidx.drawerlayout.widget.DrawerLayout>
